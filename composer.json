{
    "name": "lake/larke-admin",
    "description": "An admin api library with JWT and RBAC.",
    "keywords": [
        "laravel",
        "lake",
        "larke",
        "admin",
        "larke-admin"
    ],
    "homepage": "https://github.com/deatil/larke-admin",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "deatil",
            "email": "deatil@github.com",
            "homepage": "https://github.com/deatil",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0",
        "composer/semver": "^3.2",
        "phpseclib/phpseclib": "^3.0",
        "ramsey/uuid": "^4.3",
        "lake/larke-pclzip": "^1.1",
        "lake/larke-jwt": "^1.3",
        "lake/larke-auth": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Larke\\Admin\\": "src"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra" : {
        "laravel" : {
            "providers" : [
                "Larke\\Admin\\ServiceProvider"
            ]
        }
    }
}
